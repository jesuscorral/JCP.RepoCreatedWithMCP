{
  "name": "todo-api-backend",
  "version": "1.0.0",
  "description": "Backend API for TODO List MVP application",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "node test-endpoints.js",
    "test:endpoints": "node test-endpoints.js",
    "test:database": "node test-database.js",
    "db:migrate": "npx sequelize-cli db:migrate --config database/config.json --migrations-path database/migrations",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --config database/config.json --migrations-path database/migrations",
    "db:seed": "npx sequelize-cli db:seed:all --config database/config.json --seeders-path database/seeders",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all --config database/config.json --seeders-path database/seeders",
    "db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "todo",
    "api",
    "express",
    "nodejs",
    "mvp"
  ],
  "author": "jesuscorral",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
